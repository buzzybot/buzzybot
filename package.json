{
  "author": "buzzysin <buzzysin@users.noreply@github.com>",
  "description": "Monorepo for a Discord.JS bot-building 'framework'",
  "license": "ISC",
  "main": "index.js",
  "name": "buzzybot-2",
  "version": "0.0.0-alpha.0",
  "devDependencies": {
    "@types/node": "^16.7.10",
    "depcheck": "^1.4.2",
    "jest": "^27.1.0",
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "keywords": [],
  "scripts": {
    "lerna": "lerna",
    "lerna:rm": "yarn lerna:exec 'yarn remove'",
    "lerna:run": "yarn lerna run --stream",
    "lerna:exec": "yarn lerna exec --stream",
    "lernap:run": "yarn lerna:run --parallel",
    "lernap:exec": "yarn lerna:exec --parallel",
    "clean": "yarn lernap:run clean && rimraf *.log",
    "prebuild": "yarn clean",
    "build": "yarn lernap:run build:babel",
    "postbuild": "yarn lerna:run build:tsc",
    "build:check": "yarn build && echo 'Build successful' && yarn clean",
    "build:watch": "yarn lernap:run build:watch",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "private": true,
  "packageManager": "yarn@3.0.2"
}
